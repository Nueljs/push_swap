{
	"tasks": [
		{
			"type": "cppbuild",
			"label": "C/C++: gcc build active file",
			"command": "/usr/bin/gcc",
			"args": [
				"-fdiagnostics-color=always",
				"-g",
				"${file}",
				"-o",
				"${fileDirname}/${fileBasenameNoExtension}"
			],
			"options": {
				"cwd": "${fileDirname}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"detail": "Task generated by Debugger."
		},
		{
			"type": "shell",
			"label": "build push_swap",
			"command": "gcc -g -Wall -Werror -Wextra -I. -Ift_printf push_swap.c ft_printf/*.c utils/*.c -o push_swap",
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": "build",
			"detail": "Compila todos los archivos .c con s√≠mbolos de debug"
		}
	],
	"version": "2.0.0"
}